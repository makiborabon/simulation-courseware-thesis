			#main-body{
				height: 99.8%;
				width: 99.8%;
				background-image:url("../images/design/lgbg.jpg");
				background-repeat: no-repeat;
				background-size: 100% 100%;

			}
			#navigator{

				height: 5%;
				width: 50%;
				position: relative;
				top: 20%;
				left: 25%;
				padding: 0px;
			}
			#navigator > ul > li{
				color: e6e7e9;
				list-style: none;
				text-decoration: none;
				display: inline;
				font-size: 20pt;
			}
			#navigator > ul > li >a{
				color: black;
				list-style: none;
				text-decoration: none;
				background-color: e6e7e9;
				padding: 7px;
				border-radius: 5px 5px;

			}

			#navigator > ul > li >#selected{
				background-color: 00a4da;
				color: e6e7e9;
			}
			#navigator > ul > li > a:hover{
				color: red;
			}
